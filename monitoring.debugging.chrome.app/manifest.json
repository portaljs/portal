{
  "name": "Portal Debugging/Monitoring",
  "description": "Debugging for Portal",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
		"default_title": "",
    "default_popup": "window.html",
    "default_icon": "icon.png" },
  "background": {
		"scripts": ["monitor.js"] },
	"permissions": [
		"background",
		"geolocation",
		"notifications",
		"unlimitedStorage" ],
	"icons": {
		"128": "icon.png" } }